{
    "name": "@scandipwa/material-ui",
    "version": "0.0.0",
    "license": "OSL-3.0",
    "main": "./src/index.ts",
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@types/virtual-module__ui": "0.0.0",
        "fontsource-roboto": "^3.0.3"
    },
    "peerDependencies": {
        "@scandipwa/framework": "0.0.0"
    },
    "mosaic": {
        "type": "extension",
        "preference": "@virtual-module/ui",
        "extensions": {
            "@scandipwa/framework": false
        }
    },
    "eslintConfig": {
        "extends": "@scandipwa",
        "rules": {
            "@scandipwa/scandipwa-guidelines/only-render-in-component": "off",
            "@scandipwa/scandipwa-guidelines/jsx-no-props-destruction": "off"
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
